{
  "name": "angular-signup-verification-boilerplate-2024",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npx ng serve --open",
    "start:dev": "npx ng serve --configuration=development",
    "build": "npx ng build --configuration=production",
    "build:prod": "npx ng build --configuration=production --aot --build-optimizer --optimization --source-map=false",
    "build:dev": "npx ng build --configuration=development",
    "build:render": "npm ci --include=dev --legacy-peer-deps && npm run build:prod",
    "test": "npx ng test",
    "lint": "npx ng lint",
    "clean": "rm -rf dist node_modules/.cache",
    "clean:all": "rm -rf dist node_modules package-lock.json && npm install",
    "clean:install": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps",
    "postinstall": "echo 'Frontend dependencies installed successfully'"
  },
  "private": true,
  "overrides": {
    "debug": "4.1.1",
    "https-proxy-agent": "5.0.1",
    "agent-base": "6.0.2",
    "ms": "2.1.3",
    "supports-color": "8.1.1",
    "inherits": "2.0.4",
    "readable-stream": "3.6.2",
    "chalk": "4.1.2"
  },
  "resolutions": {
    "debug": "4.1.1",
